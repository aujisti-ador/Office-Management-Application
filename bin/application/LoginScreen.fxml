<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="252.0" prefWidth="414.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginScreenController">
   <children>
      <Label fx:id="isConnected" layoutX="66.0" layoutY="30.0" prefHeight="59.0" prefWidth="289.0" text="Enter Username and Password to access" textFill="#dd0909">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="txtUsername" layoutX="107.0" layoutY="89.0" prefHeight="36.0" prefWidth="205.0" promptText="Username">
         <font>
            <Font size="14.0" />
         </font></TextField>
      <PasswordField fx:id="txtPassword" layoutX="108.0" layoutY="138.0" prefHeight="36.0" prefWidth="205.0" promptText="Password">
         <font>
            <Font size="14.0" />
         </font></PasswordField>
      <Button layoutX="161.0" layoutY="202.0" mnemonicParsing="false" onAction="#Login" prefHeight="36.0" prefWidth="92.0" text="Login">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Label fx:id="status" layoutX="193.0" layoutY="6.0" text="Label" />
   </children>
</AnchorPane>
